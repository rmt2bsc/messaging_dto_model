<?xml version="1.0"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" >
	<xsd:include schemaLocation="../misc/Misc_Types_Type.xsd"/>
	
	<xsd:complexType name="auth_criteria_group_type">
		<xsd:sequence>
			<xsd:element name="user_criteria" type="user_criteria_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>User profile selection criteria</xsd:documentation> 
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="user_app_roles_criteria" type="user_app_roles_criteria_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>User application roles criteria</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="resource_criteria" type="resource_criteria_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Resource selection criteria</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="user_criteria_type">
		<xsd:annotation>
			<xsd:documentation>The selection criteria for user profile search</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="login_id" type="xsd:int" minOccurs="0" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation>User's Primary Key value to search by</xsd:documentation> 
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="user_name" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation>User's login id to search by</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="first_name" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation>User's first name to search by</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="last_name" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation>User's last name to search by</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="email" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation>User's email address to search by</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="dob" type="xsd:date" minOccurs="0" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation>Date of birth of the user to search by.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ssn" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation>The user's social security number to search by.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="start_date" type="xsd:date" minOccurs="0" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation>The date and time user's security profile was created to search by.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="term_date" type="xsd:date" minOccurs="0" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation>The date and time the user's security profile was terminated to search by.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="active" type="xsd:boolean" minOccurs="0" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation>Indicates whether user's security profile is currently active to search by.  A value of true equals user is active.   An inactive user status will equal false.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="group_id" type="xsd:int" minOccurs="0" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation>User group id</xsd:documentation> 
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="group_name" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation>User group name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="user_app_roles_criteria_type">
		<xsd:sequence>
			<xsd:element name="user_app_role_id" type="xsd:int" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The unique id of the user's application role id</xsd:documentation> 
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="login_id" type="xsd:int" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>User's login id as search criteria</xsd:documentation> 
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="user_name" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">The user's login id to obtain application roles.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="first_name" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">The user's first name.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="last_name" type="xsd:string" minOccurs="0" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">The user's last name.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_id" type="xsd:int" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The id of the application.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_code" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The code name of the application.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_description" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The description of the application.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_active" type="xsd:int" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>A flag to indicate whether or not an application is active.   1=true and 0=false.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="role_id" type="xsd:int" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The id of the user's role.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="role_name" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The name of the user's role</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="role_description" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The description of the user's role</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_role_id" type="xsd:int" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The id of the application role.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_role_code" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The name of the application role</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_role_name" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The name of the application role</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_role_description" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The description of the application role</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="resource_criteria_type">
		<xsd:annotation>
			<xsd:documentation>This is the document that manages user resource, resource type and resource sub type elements.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="rsrc_id" type="xsd:int"  minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the user resource unique id</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="rsrc_name" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the user resource name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="rsrc_description" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the user resource description</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="rsrc_type_id" type="xsd:int"  minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the id of the resource type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="rsrc_type_name" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the name of the resource type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="rsrc_subtype_id" type="xsd:int"  minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the id of the resource sub type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="rsrc_subtype_name" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the name of the resource sub type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="rsrc_subtype_description" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the description of the resource sub type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="secured" type="xsd:int" minOccurs="0" maxOccurs="1" default="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether or not the user resource is secured</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="rsrc_access_id" type="xsd:int" minOccurs="0" maxOccurs="1" default="0">
				<xsd:annotation>
					<xsd:documentation>Resource access id</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="login_id" type="xsd:int" minOccurs="1" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation>User's Primary Key value to search by</xsd:documentation> 
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="group_id" type="xsd:int" minOccurs="0" maxOccurs="1" nillable="false">
				<xsd:annotation>
					<xsd:documentation>User group id</xsd:documentation> 
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="auth_profile_group_type">
		<xsd:sequence>
			<xsd:element name="application_info" type="application_type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Manages application data.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="role_info" type="role_type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>The user's granted and revoked roles.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_role_info" type="app_role_type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Manages application roles.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="user_group_info" type="user_group_type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Manages user groups.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="user_app_role_info" type="user_app_role_type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Manages user application roles.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="user_info" type="user_type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>Manages users.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="resources_info" type="resources_info_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Manages user resources, user resource type, and user resource sub type datat.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="application_access_info" type="application_access_type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation xml:lang="en">Manages a list of the user's application access configuration.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="resources_info_type">
	    <xsd:annotation>
			<xsd:documentation>This is the document that manages user resource, resource type and resource sub type elements.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="resource" type="resource_type"  minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>This is the user resource unique id</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="resourcetype" type="resourcetype_type"  minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>This is the id of the resource type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="resourcesubtype" type="resourcesubtype_type"  minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>This is the id of the resource sub type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="resource_type">
		<xsd:annotation>
			<xsd:documentation>This is the document that manages user resource objects.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="uid" type="xsd:int"  minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the user resource unique id</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="code" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the user resource code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="description" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the user resource description</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="type_info" type="resourcetype_type"  minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the id of the resource type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="subtype_info" type="resourcesubtype_type"  minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the id of the resource sub type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="url" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The URL used to invoke the user resource</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="host" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The host where the user resource lives</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="secured" type="xsd:int" minOccurs="0" maxOccurs="1" default="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether or not the user resource is secured</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tracking" type="record_tracking_type" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="resourcetype_type">
		<xsd:annotation>
			<xsd:documentation>This is the document that manages user resource type objects.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="uid" type="xsd:int"  minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the id of the resource type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="description" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the name of the resource type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tracking" type="record_tracking_type" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="resourcesubtype_type">
		<xsd:annotation>
			<xsd:documentation>This is the document that manages user resource sub type objects.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="uid" type="xsd:int"  minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the id of the resource sub type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="type_info" type="resourcetype_type"  minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the id of the assoicated resource type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="code" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the name of the resource sub type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="description" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is the description of the resource sub type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tracking" type="record_tracking_type" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	
    <xsd:complexType name="role_type">
		<xsd:annotation>
			<xsd:documentation>This is the document that manages roles.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="role_id" type="xsd:int" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The id of the user's role.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="role_name" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The name of the user's role</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="role_description" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The description of the user's role</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tracking" type="record_tracking_type" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="application_type">
		<xsd:annotation>
			<xsd:documentation>This is the document that manages application data.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="app_id" type="xsd:int" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The id of the application.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_code" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The code name of the application.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="description" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The description of the application.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="active" type="xsd:int" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Status of the application.  Active equals true and Inactive equals false.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tracking" type="record_tracking_type" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>

    <xsd:complexType name="app_role_type">
		<xsd:annotation>
			<xsd:documentation>This is the document that manages application roles.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="app_role_id" type="xsd:int" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The application role id</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_info" type="application_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The base application id</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="role_info" type="role_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The base application id</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_role_code" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The application role code</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_role_name" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The application role name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_role_desc" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The application role description</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tracking" type="record_tracking_type" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="user_group_type">
		<xsd:sequence>
			<xsd:element name="grp_id" type="xsd:int" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>An id that uniquely identifies a user group.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="description" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The description of the user group</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tracking" type="record_tracking_type" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="user_type">
		<xsd:annotation>
			<xsd:documentation>The profile of a user</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="login_id" type="xsd:int" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>User's Primary Key value</xsd:documentation> 
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="group_info" type="user_group_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>User's group data</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="user_name" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>User's login id</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="first_name" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>User's first name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="last_name" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>User's last name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="short_name" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>User's name in the format: [last name, first name]</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="email" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>User's email address</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="logged_in" type="xsd:int" minOccurs="0" maxOccurs="1" default="0">
				<xsd:annotation>
					<xsd:documentation>Indicates whether user is currently logged into the system.  A value of true equals user is logged in.   Otherwise, the indicator will equal false.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="dob" type="xsd:date" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Date of birth of the user.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="total_logons" type="xsd:int" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The total number of times the user has successfully logged into the system.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ssn" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The user's social security number.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="start_date" type="xsd:date" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The date and time user's security profile was created.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="term_date" type="xsd:date" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The date and time the user's security profile was terminated.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="active" type="xsd:int" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Indicates whether user's security profile is currently active.  A value of true equals user is active.   An inactive user status will equal false.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="description" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>A description of the user's profile.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="password" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The user's password.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="session_id" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The user's session id or security token.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="granted_app_roles" type="user_app_roles_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is a list of application roles that have been granted to said user</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="revoked_app_roles" type="user_app_roles_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is a list of application roles that have been revoked from the said user.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="granted_resources" type="user_resource_access_list_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is a list of resources that have been granted to said user</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="revoked_resources" type="user_resource_access_list_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>This is a list of resources that have been revoked from the said user.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tracking" type="record_tracking_type" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="user_resource_access_list_type">
		<xsd:sequence>
			<xsd:element name="user_resource_access" type="user_resource_access_type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>list of user resource access types.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="user_resource_access_type">
		<xsd:sequence>
			<xsd:element name="user_rsrc_access_id" type="xsd:int" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>An id that uniquely identifies a user's resourcde access profile.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="user_info" type="user_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>User Type Information</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="user_group_info" type="user_group_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>User group informaion</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="resource_info" type="resource_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Resource type informaion</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tracking" type="record_tracking_type" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="user_app_roles_type">
		<xsd:sequence>
			<xsd:element name="user_app_role" type="user_app_role_type" minOccurs="0" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>One or more user application role type objects.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="user_app_role_type">
		<xsd:sequence>
			<xsd:element name="user_app_role_id" type="xsd:int" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>An id that uniquely identifies a user's application role.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="user_info" type="user_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>User Type Information</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_role_info" type="app_role_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Application role to associated with the user</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="tracking" type="record_tracking_type" minOccurs="0" maxOccurs="1"/>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="application_access_type">
		<xsd:sequence>
			<xsd:element name="app_access_id" type="xsd:int" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>An id that uniquely identifies an application access entity.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="user_info" type="user_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>User Type Information</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_info" type="application_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Application data</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="logged_in" type="xsd:int" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>A flag indicating whether or not the user is currently logged in.  1=yes, 0=no.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="login_date" type="xsd:dateTime" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The date and time the user logged into the system.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="last_login_date" type="xsd:dateTime" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The date and time of the user's last login.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="session_id" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The user's session id or security token.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="user_login_type">
		<xsd:annotation>
			<xsd:documentation>This is the document that manages user's  login credentials.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="uid" type="xsd:string" default="0" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The user's login id.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="pw" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The user's password.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_code" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The Application code name.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="session_id" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The user's session id.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="user_logout_type">
		<xsd:annotation>
			<xsd:documentation>This is the document that manages user's logout credentials.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="uid" type="xsd:string" default="0" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The user's login id.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="session_id" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The user's session id.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="user_session_type">
		<xsd:annotation>
			<xsd:documentation>This is the document that manages the user's session data.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="login_id" type="xsd:string" default="0" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The user's login id.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="ws_token" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>A security token that is used by the user to access web services once successfully logged in.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="session_id" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The user's session id.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="auth_session_id" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The session id of the application performing authentication for the user.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fname" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The user's first name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="lname" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The user's last name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="access_level" type="xsd:int" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The access level</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="gateway_interface" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The gateway interface</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="remote_host" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The remote host the user received its authentication</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="remote_addr" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The remote host's IP address</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="remote_app_name" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The remote host's application code name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="server_protocol" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The protocol used</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="server_software" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The server software</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="server_name" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The server name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="server_port" type="xsd:int" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The server port</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="user_agent" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>User agent</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="local" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The local</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="accept" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>accept</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="accept_lang" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>accept language</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="accept_encoding" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>accept encoding</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="scheme" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The scheme</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="server_info" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Server information</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="servlet_context" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Servlet Context</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="session_create" type="xsd:long" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>A long number representing the date and time the session was created</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="session_last_accessed" type="xsd:long" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>A long number representing the date and time the session was last active.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="session_max" type="xsd:int" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Indicates in seconds how long the session will last without any user activity before timing out</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="orig_app_id" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The original application code name</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="group_id" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The group the user belongs</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="roles" type="app_role_type" minOccurs="1" maxOccurs="unbounded">
				<xsd:annotation>
					<xsd:documentation>A list of the user's application roles</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	
	<xsd:complexType name="user_app_login_check_type">
		<xsd:annotation>
			<xsd:documentation>This is the document that manages user's  login credentials.</xsd:documentation>
		</xsd:annotation>
		<xsd:sequence>
			<xsd:element name="uid" type="xsd:string" default="0" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The user's login id.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="app_code" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The Application code name.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="session_id" type="xsd:string" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The user's session id.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="signed_on" type="xsd:boolean" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Indicates the authentication status of a user.  true indicates that the user is already signed on.  Otherwise, the user is not signed on.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="session_token" type="user_session_type" minOccurs="1" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The user's session token.</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>