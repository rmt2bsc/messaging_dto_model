		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="xact_lineitem_list_type">
		<xsd:sequence>
			<xsd:element name="lineitem" type="xact_lineitem_type" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>

	<xsd:complexType name="xact_itemtype_type">
		<xsd:sequence>
			<xsd:element name="itemtype_id" type="xsd:integer"/>
			<xsd:element name="xact_type_id" type="xact_type"/>
			<xsd:element name="name" type="xsd:string"/>
			<xsd:element name="tracking" type="record_tracking_type" minOccurs="0" maxOccurs="1"/>
