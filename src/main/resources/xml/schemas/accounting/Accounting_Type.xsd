	<xsd:complexType name="transaction_criteria_group">
		<xsd:sequence>
			<xsd:element name="gl_criteria" type="gl_criteria_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The General Ledger selection criteria</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="customer_criteria" type="customer_criteria_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The customer selection criteria</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="creditor_criteria" type="creditor_criteria_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The creditor criteria</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="creditortype_criteria" type="creditortype_criteria_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The creditor type</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="sales_criteria" type="sales_order_criteria" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The sales order criteria</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="purchases_criteria" type="purchase_order_criteria" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>The purchases criteria</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="xact_criteria" type="xact_criteria_type" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>Basic and custom transaction criteria</xsd:documentation>
				</xsd:annotation>
